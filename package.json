{
  "name": "nested-json-formatter",
  "displayName": "Nested JSON Formatter",
  "description": "%extension.description%",
  "version": "1.0.5",
  "publisher": "wykdg",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/wykdg/nested-json-vscode.git"
},
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "scripts": {
    "compile": "tsc -p ./",
    "package": "vsce package",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "@types/vscode": "^1.80.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "nestedJson.format",
        "title": "%command.format.title%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "nestedJson.format",
          "when": "editorLangId == json",
          "group": "1_modification"
        }
      ],
      "editor/title": [
        {
          "command": "nestedJson.format",
          "when": "editorLangId == json",
          "group": "navigation",
          "priority": 1
        }
      ]
    }
  }
}